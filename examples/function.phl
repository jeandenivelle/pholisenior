
def function( P, Q : Form( Obj ), f : Obj( Obj )) :=
   [ x1 : Obj ] P(x1) -> Q( f(x1)) : Form;

def function( P : Form( Obj, Obj ), Q : Form( Obj ))
            ( f : Obj( Obj, Obj )) :=
   [ x1, x2 : Obj ] P( x1, x2 ) -> Q( f(x1,x2) ) : Form;

def function( P : Form( Obj, Obj, Obj ), Q : Form( Obj ))
            ( f : Obj( Obj, Obj, Obj )) :=
   [ x1, x2, x3 : Obj ] P( x1, x2, x3 ) -> Q( f( x1, x2, x3 )) : Form;

